programa
{
	funcao inicio()
	{
		inteiro a, b
		escreva("Digite dois numero: ")
		leia(a,b)

		escreva("A soma entre os numeros é ",a+b, "\n")
		escreva("A subtração entre os numeros é ",a-b, "\n")
		escreva("A multiplicação entre os numeros é ",a*b, "\n")
		escreva("A divisão entre os numeros é ",a/b, "\n")
		
	}
}
